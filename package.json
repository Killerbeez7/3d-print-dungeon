        "@types/node": "^22.15.30",
        "@types/nprogress": "^0.2.3",
        "@types/react": "^19.0.10",
        "@types/react-dom": "^19.0.4",
        "@types/react-slick": "^0.23.13",
        "@types/testing-library__react": "^10.0.1",
        "@types/three": "^0.178.0",
        "@typescript-eslint/eslint-plugin": "^8.33.1",
        "@typescript-eslint/parser": "^8.33.1",
        "@vitejs/plugin-react": "^4.3.4",
        "@vitest/ui": "^3.1.2",
