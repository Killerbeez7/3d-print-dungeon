{
    "name": "3d-print-dungeon",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "lint": "eslint .",
        "preview": "vite preview",
        "type-check": "tsc --noEmit",
        "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "hosting": "npm run generate-sitemap && npm run build && firebase deploy --only hosting",
        "generate-sitemap": "node src/scripts/sitemap.cjs",
        "firebase:prod": "node scripts/switch-firebase-config.js production",
        "firebase:emulator": "node scripts/switch-firebase-config.js emulator",
        "firebase:functions-only": "node scripts/switch-firebase-config.js emulator && firebase emulators:start --only functions"
    },
    "browserslist": [
        "defaults",
        "not IE 11",
        "not op_mini all"
    ],
    "dependencies": {
        "@fingerprintjs/fingerprintjs": "^4.6.2",
